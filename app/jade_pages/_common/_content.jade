section.change__block
	header.header__cort
		h4.sort__of Сортировать по
		.sort__select
			select.sort__select-elem
				- var options = ["по цене", "по популярности"]
				each option in options
					option(value="")= option
		ul.variants__list
			- var variants= ['six', 'three', 'nine']
			each variant in variants
				li(class="variants__item" class="variants__link_" + variant)
					a(href="#" class="variants__link" id="variants__link_" + variant)
						.sort__view-icon
		h4.variants__catalog Вид каталога
